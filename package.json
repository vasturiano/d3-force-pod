{
  "name": "d3-force-pod",
  "version": "0.9.6",
  "description": "Component to automatically draw nodes/links in a D3 force simulation according to a set of forces",
  "unpkg": "dist/d3-force-pod.min.js",
  "main": "dist/d3-force-pod.common.js",
  "module": "dist/d3-force-pod.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/d3-force-pod.git"
  },
  "keywords": [
    "d3",
    "d3-module",
    "force",
    "simulation",
    "pod",
    "graph",
    "svg"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/d3-force-pod/issues"
  },
  "homepage": "https://github.com/vasturiano/d3-force-pod",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/d3-force-pod.js -o dist/d3-force-pod.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "d3-array": "^2.4.0",
    "d3-force": "^2.0.1",
    "d3-selection": "^1.4.1",
    "kapsule": "^1.12.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-watch": "^4.3.1",
    "terser": "^4.6.6"
  }
}
