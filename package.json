{
  "name": "d3-force-pod",
  "version": "0.9.4",
  "description": "Component to automatically draw nodes/links in a D3 force simulation according to a set of forces",
  "unpkg": "dist/d3-force-pod.min.js",
  "main": "dist/d3-force-pod.common.js",
  "module": "dist/d3-force-pod.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/d3-force-pod.git"
  },
  "keywords": [
    "d3",
    "d3-module",
    "force",
    "simulation",
    "pod",
    "graph",
    "svg"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/d3-force-pod/issues"
  },
  "homepage": "https://github.com/vasturiano/d3-force-pod",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c",
    "watch": "rollup -c -w",
    "minify": "uglifyjs dist/d3-force-pod.js -o dist/d3-force-pod.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepublish": "npm run build && npm run minify && npm run build-module"
  },
  "dependencies": {
    "d3-array": "^1.2.1",
    "d3-force": "^1.1.0",
    "d3-selection": "^1.1.0",
    "kapsule": "^1.7.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.4",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.3.1",
    "uglify-js": "^3.1.5"
  }
}
